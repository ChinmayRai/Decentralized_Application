struct parkingLot {
		address owner;
		location lat;
		location long;
		string name;
		parkingSpot[] spots;
		uint num;
		uint price;
		uint size; // 0 = small, 1 = medium, 2 = big
	}

	struct parkingSpot{
		bool status;
		address renter;
		uint duration;
		uint startTime;
		uint [] BookingIndex;
	}

